# sum_of_two_numbers.py

def add_two_numbers(a, b):
    return a + b

# Example usage
num1 = int(input("Enter first number: "))
num2 = int(input("Enter second number: "))
result = add_two_numbers(num1, num2)
print("Sum:", result)
